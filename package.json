{
  "name": "ms-session-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm run envLocal && npm run db && nodemon --exec babel-node src/index.js",
    "build": "npm run clean && babel src -d dist",
    "clean": "rimraf dist",
    "db": "babel-node ./db/index.js",
    "envLocal": "cp .env-local .env",
    "envDocker": "cp .env-docker .env",
    "start": "npm run envDocker && npm run clean && npm run build && npm run db && cd dist && node index.js",
    "test": "npm run envLocal && npm run db && vitest"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.21.4",
    "nodemon": "^2.0.22",
    "rimraf": "^4.4.1",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.0",
    "amqplib": "^0.10.3",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.2.0",
    "redis": "^4.6.6"
  }
